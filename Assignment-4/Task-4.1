# Prompt: Write a Python function that checks whether a given year is a leap year.
def is_leap_year(year: int) -> bool:
    """Return True if the given year is a leap year, False otherwise."""
    if year % 4 != 0:
        return False
    elif year % 100 != 0:
        return True
    elif year % 400 != 0:
        return False
    else:
        return True
