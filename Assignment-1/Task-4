# Recursive function to calculate factorial of n
def factorial(n):
    if n == 0 or n == 1:
        return 1
    else:
        return n * factorial(n - 1)
    # Iterative function to calculate factorial of n
def factorial_iterative(n):
    result = 1
    for i in range(2, n + 1):
        result *= i
    return result
print(factorial(5))  
print(factorial_iterative(5))
print(factorial(0)) 

