# Function to find the largest number in a list
def largest_number(lst):
    if not lst:
        return None
    max_num = lst[0]
    for num in lst:
        if num > max_num:
            max_num = num
    return max_num
numbers = [10, 45, 2, 99, 23]
print(largest_number(numbers))  
empty_list = []
print(largest_number(empty_list))  
